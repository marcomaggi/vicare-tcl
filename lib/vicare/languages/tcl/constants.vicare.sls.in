(library (vicare languages tcl constants (0 4 2015 7 15))
  (export
    TCL_WINDOW_EVENTS
    TCL_FILE_EVENTS
    TCL_TIMER_EVENTS
    TCL_IDLE_EVENTS
    TCL_ALL_EVENTS
    TCL_DONT_WAIT)
  (import (rename (only (vicare)
			define-inline-constant)
		  (define-inline-constant	defc)))


;;;; constants

;; events loop flags
(defc TCL_WINDOW_EVENTS		@VALUEOF_TCL_WINDOW_EVENTS@)
(defc TCL_FILE_EVENTS		@VALUEOF_TCL_FILE_EVENTS@)
(defc TCL_TIMER_EVENTS		@VALUEOF_TCL_TIMER_EVENTS@)
(defc TCL_IDLE_EVENTS		@VALUEOF_TCL_IDLE_EVENTS@)
(defc TCL_ALL_EVENTS		@VALUEOF_TCL_ALL_EVENTS@)
(defc TCL_DONT_WAIT		@VALUEOF_TCL_DONT_WAIT@)


